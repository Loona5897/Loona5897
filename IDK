UIEditor:ColorPicker("StaminaBar ColorPicker",Color3.fromRGB(255,0,0),function(color)
    game.Players.LocalPlayer.PlayerGui.UI.Ui.StaminaBar.Bar.BackgroundColor3 = Color3.new(color)
end)
UIEditor:ColorPicker("ManaBar ColorPicker",Color3.fromRGB(255,0,0),function(color)
    game.Players.LocalPlayer.PlayerGui.UI.Ui.ManaBar.Bar.BackgroundColor3 = Color3.new(color)
end)
UIEditor:ColorPicker("HealthBar ColorPicker",Color3.fromRGB(255,0,0),function(color)
    game.Players.LocalPlayer.character.Head.HealthBar.Frame.BackgroundColor3 = Color3.new(color)
end)
UIEditor:Box("Attack Edit /Non FE","Number",function(value)
    game.Players.LocalPlayer.PlayerGui.UI.Ui.Info.Attack.Text = value
end)
UIEditor:Box("Defense Edit /Non FE","Number",function(value)
    game.Players.LocalPlayer.PlayerGui.UI.Ui.Info.Defense.Text = value
end)
UIEditor:Box("Pname Edit /Non FE","Number",function(value)
    game.Players.LocalPlayer.character.Head.HealthBar.Frame.PName.Text = value
end)
UIEditor:Button("Delete SettingsButton",function()
    game.Players.LocalPlayer.PlayerGui.UI.Ui.SettingsButton:Destroy()
end)
UIEditor:Button("Delete UpdateLog",function()
    game.Players.LocalPlayer.PlayerGui.UI.Ui.UpdateLog:Destroy()
end)
UIEditor:Button("Delete 1-8 Attack UI",function()
    game.Players.LocalPlayer.PlayerGui.UI.Ui.MoveDebounceShower:Destroy()
end)
UIEditor:Button("Delete Stamina Image",function()
    game.Players.LocalPlayer.PlayerGui.UI.Ui.StaminaBar.ImageLabel:Destroy()
end)
