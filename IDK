local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local Credits = library:CreateWindow("Credits")
local Customs = library:CreateWindow("Customs")
local Attacks = library:CreateWindow("Custom ATKS")
local Editor = library:CreateWindow("Char Editor")
local troll = library:CreateWindow("Char troll/extras")

local Open = Credits:CreateFolder("Open Me")
local Normal = Customs:CreateFolder("Normal Chars")
local Special = Customs:CreateFolder("Special Chars")
local Admin = Customs:CreateFolder("Admin Chars")
local Other = Customs:CreateFolder("Other Chars")
local ATKSS = Attacks:CreateFolder("ATKS")
local UIEditor = Editor:CreateFolder("UI Editor")
local Universal = troll:CreateFolder("Universal")
local Chara = troll:CreateFolder("GTChara")
local Betty = troll:CreateFolder("Betty")
local Frisk = troll:CreateFolder("Frisk")

